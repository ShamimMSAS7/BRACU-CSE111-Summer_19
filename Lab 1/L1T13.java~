import java.util.Scanner;
public class L1T13{
  public static void main(String[]args){
    Scanner msas=new Scanner(System.in);
    System.out.println("Enter the number");
    int num=msas.nextInt();
    for(int i=1; i<=2*num-1; i++){
      if(i>num)
        System.out.print(2*num-i);
      else
        System.out.print(i);
      
    }
    System.out.println();
  }
}