import java.util.Scanner;
public class L1T12{
  public static void main(String[]args){
    Scanner msas=new Scanner(System.in);
    System.out.println("Enter the number");
    int num=msas.nextInt();
    for(int i=1; i<=num; i++){
      for(int j=1; j<=num; j++){
        if(j>num-i)
          System.out.print(j);
        else
          System.out.print(" ");
      }
      System.out.println();
    }
  }
}